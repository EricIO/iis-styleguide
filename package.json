{
  "name": "Internetstiftelsen",
  "private": true,
  "version": "0.0.1",
  "ports": {
    "assets": "8080",
    "fractal": "3000"
  },
  "scripts": {
    "slack": "node slack.js",
    "build": "cross-env NODE_ENV=production npm run build:webpack && npm run build:fractal",
    "build:fractal": "fractal build",
    "build:webpack": "cross-env NODE_ENV=production webpack --progress --optimize-minimize",
    "dev": "npm run dev:fractal & npm run dev:webpack",
    "dev:fractal": "NODE_ENV=development fractal start -s -w",
    "dev:webpack": "webpack-dev-server --progress",
    "deploy": "npm run build:webpack && npm run build:fractal && rsync -rzhP --delete build/ deploy@sto-roxt02.co.int:/var/www/$npm_package_version",
    "release": "git push && git push --tags && npm run deploy"
  },
