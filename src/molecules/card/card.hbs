<div class="m-card{{#if is_padded}} m-card--padded{{/if}}">
	<a href="{{url}}" class="m-card__link" aria-label="{{title}}">
		<img src="/assets/images/card.png" class="m-card__image" alt="{{alt}}">
		<div class="m-card__padded">{{> @meta text="2018-10-05" }}</div>
	</a>
	<div class="m-card__padded"><a href="{{url}}" class="m-card__heading-link"><h2 class="m-card__heading-link__heading">{{title}}</h2></a></div>
	<div class="m-card__bottom">
		<div class="m-card__padded">
			{{> @tag text="Utbildning" href="#" }}
			{{> @tag text="goto10" href="#" }}
		</div>
	</div>
</div>
