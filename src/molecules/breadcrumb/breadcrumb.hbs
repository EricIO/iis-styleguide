<nav aria-label="Breadcrumb">
	<ol class="breadcrumb">
		{{> @icon id="arrow-backwards"}}
		{{#each breadcrumb_item}}
		<li class="breadcrumb__item">
			<a href="{{url}}" class="breadcrumb__link" {{#if is_current}}aria-current="page"{{/if}}>
				{{title}}
			</a>
			{{> @icon id="arrow-forwards"}}
		</li>
		{{/each}}
	</ol>
</nav>
